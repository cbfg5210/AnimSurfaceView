<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimSurfaceView">
        <!--图片资源数组-->
        <attr name="imageArray" format="reference" />
        <!--是否自动启动动画-->
        <attr name="autoStart" format="boolean|reference" />
        <!--画面时间间隔-->
        <attr name="interval" format="integer" />
    </declare-styleable>
</resources>